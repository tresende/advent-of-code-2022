{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "strict": false,
    "strictPropertyInitialization": false,
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "skipLibCheck": true,
    "lib": ["ES2021"],
    "resolveJsonModule": true,
    "forceConsistentCasingInFileNames": true,
    "types": ["@types/node"]
  }
}
